{
  "compilerOptions": {
    "target": "ES2018",
    "lib": ["ESNext", "DOM"],
    "allowJs": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": false,
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "outDir": "qq_plus/renderer",
    "skipLibCheck": true,
  },
  "include": ["src/renderer/*.ts"]
}
